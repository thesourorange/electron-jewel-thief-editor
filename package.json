{
  "name": "electron-jewel-thief-editor",
  "version": "1.0.0",
  "description": "Electron Jewel Thief Editor",
  "main": "main.js",
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-jewel-thief-editor --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.ProductName=\"Jewel Thief Editor\"",
    "package-linux": "electron-packager . electron-jewel-thief-editor --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thesourorange/electron-jewel-thief-editor.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "Jewel Thief",
    "game"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "electron-pug": "1.5.1",
    "jquery": "3.2.1",
    "d3": "^3.5.6",
    "electron": "~1.7.5",
    "electron-packager": "^12.0.2",
    "electron-winstaller": "^2.6.4",
    "electron-disable-file-drop": "^1.0.2"
  },
  "bugs": {
    "url": "https://github.com/thesourorange/electron-jewel-thief-editor/issues"
  },
  "homepage": "https://github.com/thesourorange/electron-jewel-thief-editor#readme"
}
